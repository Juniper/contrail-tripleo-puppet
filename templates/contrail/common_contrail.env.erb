CLOUD_ORCHESTRATOR=openstack
OPENSTACK_VERSION=queens
AUTH_MODE=keystone
KEYSTONE_AUTH_URL_VERSION=/v3
KEYSTONE_AUTH_PROTO=<%= @auth_protocol %>
KEYSTONE_AUTH_HOST=<%= @auth_host %>
KEYSTONE_AUTH_REGION_NAME=<%= @keystone_region %>
KEYSTONE_AUTH_PROJECT_DOMAIN_NAME=<%= @keystone_project_domain_name %>
KEYSTONE_AUTH_USER_DOMAIN_NAME=<%= @keystone_user_domain_name %>
KEYSTONE_AUTH_ADMIN_TENANT=<%= @admin_tenant_name %>
KEYSTONE_AUTH_ADMIN_USER=<%= @admin_user %>
KEYSTONE_AUTH_ADMIN_PASSWORD=<%= @admin_password %>
<% if @auth_protocol != 'http' %>
KEYSTONE_AUTH_INSECURE=True
<% end %>

<% if @ssl_enabled %>
SSL_ENABLE=True
XMPP_SSL_ENABLE=True
SANDESH_SSL_ENABLE=True
INTROSPECT_SSL_ENABLE=True
INTROSPECT_SSL_INSECURE=True
SERVER_CERTFILE=<%= @cert_file %>
SERVER_KEYFILE=<%= @key_file %>
SERVER_CA_CERTFILE=<%= @ca_file %>
SERVER_CA_KEYFILE=<%= @ca_key_file %>
<% end %>

ANALYTICS_NODES=<%= @issu_ips_list_comma %>
CONTROL_NODES=<%= @issu_ips_list_comma %>
DNS_NODES=<%= @issu_ips_list_comma %>

VROUTER_GATEWAY=<%= @vrouter_gateway %>
BGP_ASN=<%= @router_asn %>
BGP_AUTO_MESH=<%= @ibgp_auto_mesh %>
